<Window x:Class="SpaceShip.Log_in"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceShip"
        mc:Ignorable="d"
         Title="log-in" Height="515" Width="540" Icon="images/player.png">
    
    <Canvas x:Name="cnv3" Background="White">

        <Button x:Name="start" Canvas.Left="201" Canvas.Top="368" RenderTransformOrigin="0.519,9.521" Width="109" Click="start_Click">
            Start
        </Button>
        <TextBox x:Name="tb1" Height="23" Canvas.Left="190" TextWrapping="Wrap" Text="" Canvas.Top="192" Width="120" TextChanged="TextBox_TextChanged"/>
        <Label Content="Enter your name" Background ="Purple" Canvas.Left="190" Canvas.Top="155" Width="120"/>
    </Canvas>
</Window>
